        "BuildParameters": {
          "ResourceKey": "!!ENTER_YOUR_RESOURCE_KEY_HERE!!",
          "CloudRequestOrigin": "51Degrees.example.com",
          // Explicitly include just the properties used by the example.
          "Properties": "HardwareVendor,HardwareModel,HardwareName,IsMobile,JavascriptGetHighEntropyValues,Promise,Fetch,DeviceType,PlatformVendor,PlatformName,PlatformVersion,BrowserVendor,BrowserName,BrowserVersion,ScreenPixelsWidth,ScreenPixelsHeight,JavascriptScreenPixelsWidth,JavascriptScreenPixelsHeight,JavascriptHardwareProfile"
